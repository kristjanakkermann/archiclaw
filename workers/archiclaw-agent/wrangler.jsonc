{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "archiclaw-agent",
  "main": "src/server.ts",
  "compatibility_date": "2025-12-17",
  "compatibility_flags": ["nodejs_compat"],
  "assets": { "directory": "public" },
  "durable_objects": {
    "bindings": [{ "name": "ArchiClawChat", "class_name": "ArchiClawChat" }],
  },
  "migrations": [{ "tag": "v1", "new_sqlite_classes": ["ArchiClawChat"] }],
  "vars": {
    "MODEL": "gpt-4o",
    "ENVIRONMENT": "production",
  },
  "env": {
    "staging": {
      "name": "archiclaw-agent-staging",
      "vars": {
        "MODEL": "gpt-4o-mini",
        "ENVIRONMENT": "staging",
      },
    },
  },
  "observability": { "enabled": true },
}
